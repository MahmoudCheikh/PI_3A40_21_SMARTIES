{% extends 'base.html.twig' %}

{% block title %}Abonnement index{% endblock %}

{% block body %}
<h3><i class="fa fa-angle-right"></i> Abonnement index</h3>
<a href="{{ path('abonnement_new') }}">Create new</a>
<div class="row mb">
    <!-- page start-->
    <div class="content-panel">
        <div class="adv-table">
            <table cellpadding="0" cellspacing="0" border="0" class="display table table-bordered" id="hidden-table-info">
        <thead>
            <tr>
                <th>Id</th>
                <th>Type</th>
                <th>Dated</th>
                <th>Datef</th>
                <th>Prix</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for abonnement in abonnements %}
            <tr>
                <td>{{ abonnement.id }}</td>
                <td>{{ abonnement.type }}</td>
                <td>{{ abonnement.dated ? abonnement.dated|date('Y-m-d') : '' }}</td>
                <td>{{ abonnement.datef ? abonnement.datef|date('Y-m-d') : '' }}</td>
                <td>{{ abonnement.prix }}</td>
                <td>
                    <a href="{{ path('abonnement_show', {'id': abonnement.id}) }}">show</a>
                    <a href="{{ path('abonnement_edit', {'id': abonnement.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
            </table>
        </div>
    </div>
    <!-- page end-->
</div>
    <!-- /row -->
{% endblock %}
